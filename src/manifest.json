{
	"manifest_version": 3,
	"name": "Your Extension Name",
	"version": "1.0",
	"description": "Turns white and black vision on",
	"homepage_url": "https://screenhue.com",
	"icons": {
		"16": "icon-bw-19.png",
		"48": "icon-bw-38.png",
		"128": "icon-bw-38.png"
	},
	"action": {
		"default_icon": {
			"19": "icon-bw-19.png",
			"38": "icon-bw-38.png"
		}
	},
	"background": {
		"service_worker": "event-page.js"
	},
	"permissions": [
		"tabs",
		"scripting",
		"activeTab",
		"storage"
	],
	"web_accessible_resources": [
		{
			"resources": ["black-and-white.css"],
			"matches": ["<all_urls>"]
		}
	],
	"host_permissions": [
		"<all_urls>"
	]
}